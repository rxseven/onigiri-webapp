{
  "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
  "coveragePathIgnorePatterns": [
    "<rootDir>/.vscode/",
    "<rootDir>/build/",
    "<rootDir>/config/",
    "<rootDir>/coverage/",
    "<rootDir>/flow-type/",
    "<rootDir>/node_modules/",
    "<rootDir>/public/",
    "<rootDir>/scripts/",
    "<rootDir>/src/__mock__/",
    "<rootDir>/src/__tests__/",
    "<rootDir>/src/config/",
    "<rootDir>/src/constants/",
    "<rootDir>/src/sagas/",
    "<rootDir>/src/services/",
    "<rootDir>/src/store/",
    "<rootDir>/src/styles/",
    "<rootDir>/src/tests/",
    "<rootDir>/src/types/",
    "<rootDir>/src/dependencies.js",
    "<rootDir>/src/index.js",
    "<rootDir>/src/data/sagas.js",
    "<rootDir>/src/helpers/ajax.js",
    "<rootDir>/src/screens/sagas.js",
    "<rootDir>/src/screens/Users/sagas.js"
  ],
  "moduleFileExtensions": ["web.js", "js", "json", "web.jsx", "jsx", "node", "mjs"],
  "moduleNameMapper": {
    "^react-native$": "react-native-web"
  },
  "setupFiles": ["<rootDir>/config/polyfills.js", "jest-localstorage-mock"],
  "setupTestFrameworkScriptFile": "<rootDir>/src/tests/setup.js",
  "testEnvironment": "node",
  "testMatch": [
    "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
    "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/.vscode/",
    "<rootDir>/build/",
    "<rootDir>/config/",
    "<rootDir>/coverage/",
    "<rootDir>/flow-type/",
    "<rootDir>/node_modules/",
    "<rootDir>/public/",
    "<rootDir>/scripts/",
    "<rootDir>/src/__mock__/",
    "<rootDir>/src/__tests__/",
    "<rootDir>/src/config/",
    "<rootDir>/src/constants/",
    "<rootDir>/src/sagas/",
    "<rootDir>/src/services/",
    "<rootDir>/src/store/",
    "<rootDir>/src/styles/",
    "<rootDir>/src/tests/",
    "<rootDir>/src/types/"
  ],
  "testURL": "http://localhost",
  "transform": {
    "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
    "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
    "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
  },
  "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"]
}
